pipeline {
    agent any

    tools {
        maven 'mymaven'
        jdk 'java17'
    }

    stages {
        stage('Compile') {
            steps {
                echo 'Compiling the code'
                sh 'mvn clean compile'
            }
        }

        stage('Code Review') {
            steps {
                echo 'Code review passed'
            }
        }

        stage('Unit Test') {
            steps {
                sh 'mvn test'
            }
        }

        stage('Coverage Analysis') {
            steps {
                echo 'Coverage analysis completed'
            }
        }

        stage('Package') {
            steps {
                sh 'mvn package'
            }
        }
    }
}
